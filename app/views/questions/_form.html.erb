<%= render("layouts/errors", :object => @question) %>

<%= form_for [@question.user, @question] do |f| %>
  <div class="form-group">
    <%= f.label :question %>
    <%= f.text_field :question %>
  </div>
  <div class="form-group">
    <%= f.label :description %>
    <%= f.text_area :description %>
  </div>
  <%= f.submit(:class => "btn btn-primary") %>
<% end %>
